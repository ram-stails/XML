	<LOGIC_DEFINITION>

		<IF>
			<EQ expr1="FIELD(Appliance_Selection).value" expr2="1" />
			<THEN>
				<COMMAND>"FIELD(Base_Unit_Height).value=720"</COMMAND>
				<COMMAND>"FIELD(TU_Appliance_Selection).value=FIELD(Combination_1).value"</COMMAND>
				<COMMAND>"FIELD(Combination_Type_1).visible=TRUE"</COMMAND>
				<COMMAND>"FIELD(Combination_Type_2_1).visible=FALSE"</COMMAND>
				<COMMAND>"FIELD(Combination_Type_2_2).visible=FALSE"</COMMAND>
				<COMMAND>"FIELD(Combination_Type_3_1).visible=FALSE"</COMMAND>
