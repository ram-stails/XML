				<COMMAND>"FIELD(Combination_Type_3_3).visible=FALSE"</COMMAND>
				<COMMAND>"FIELD(Appliance_Height).value=FIELD(Combination_1).info1"</COMMAND>
			</THEN>
			<ELSE>
				<IF>
					<EQ expr1="FIELD(Appliance_Selection).value" expr2="2" />
					<THEN>
						<COMMAND>"FIELD(Base_Unit_Height).value=360"</COMMAND>
						<COMMAND>"FIELD(TU_Appliance_Selection).value=TU_HosingUnit_2Appliances"</COMMAND>
						<COMMAND>"FIELD(Combination_Type_1).visible=FALSE"</COMMAND>
						<COMMAND>"FIELD(Combination_Type_2_1).visible=TRUE"</COMMAND>
						<COMMAND>"FIELD(Combination_Type_2_2).visible=TRUE"</COMMAND>
						<COMMAND>"FIELD(Combination_Type_3_1).visible=FALSE"</COMMAND>
						<COMMAND>"FIELD(Combination_Type_3_2).visible=FALSE"</COMMAND>
						<COMMAND>"FIELD(Combination_Type_3_3).visible=FALSE"</COMMAND>						
						<COMMAND>"FIELD(TU_Appliance_Selection_1).value=FIELD(Combination_2_1).value"</COMMAND>
						<COMMAND>"FIELD(TU_Appliance_Selection_2).value=FIELD(Combination_2_2).value"</COMMAND>
						<COMMAND>"FIELD(Appliance_Height_1).value=FIELD(Combination_2_1).info1"</COMMAND>
						<COMMAND>"FIELD(Appliance_Height_2).value=FIELD(Combination_2_2).info1"</COMMAND>
						<COMMAND>"FIELD(Appliance_Height).value=CALC(FIELD(Appliance_Height_1).value+FIELD(Appliance_Height_2).value)"</COMMAND>
					</THEN>
					<ELSE>
						<IF>
							<EQ expr1="FIELD(Appliance_Selection).value" expr2="3" />
							<THEN>
								<COMMAND>"FIELD(Base_Unit_Height).value=360"</COMMAND>
								<COMMAND>"FIELD(TU_Appliance_Selection).value=TU_HosingUnit_3Appliances"</COMMAND>
