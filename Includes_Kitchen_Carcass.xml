				<ITEM value="GreenOptimaG" label="Optima G" info1="GreenPly" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="GreenEcotech" label="Ecotech" info1="GreenPly" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="ClubPrime" label="Club Prime" info1="Century" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="Bond710" label="Bond710" info1="Century" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="Sainik710" label="Sainik710" info1="Century" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="SainikMR" label="Sainik MR" info1="Century" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="SainikMR" label="Sainik MR" info1="Century" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="RamMDF" label="Raw MDF" info1="ActionTesa" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="RawHDHMR" label="Raw HDHMR" info1="ActionTesa" info2="postLam" image1="img/material/BWP.jpg" />
				<ITEM value="ParticalBoard" label="PreLam Partical Board" info1="ActionTesa" info2="preLam" image1="img/material/BWP.jpg" />
				<ITEM value="FrostyWhite101" label="FrostyWhite101" info1="ActionTesa" info2="preLam" image1="img/material/FrostyWhite101.jpg" />
				<ITEM value="SlateGrey105" label="SlateGrey105" info1="ActionTesa" info2="preLam" image1="img/material/SlateGrey105.jpg" />

			</COMBOBOX>

			<COMBOBOX name="CarcassLam" label="Carcass Lam" valuetype="image" visible="TRUE" active="TRUE" image1="img/catalog/.PNG" search="true">
				<ITEM value="FrostyWhite" label="Merino - Frosty White" info1="postLam" image1="img/material/BWR.jpg" />
				<ITEM value="Grey" label="Merino - Grey" info1="postLam" image1="img/material/BWR.jpg" />
				<ITEM value="FrostyWhite1" label="Saburi - Frosty White" info1="preLam" image1="img/material/BWR.jpg" />
				<ITEM value="Grey1" label="Saburi - Grey" info1="preLam" image1="img/material/BWR.jpg" />

			</COMBOBOX>
		</GROUP>

		<GROUP name="Carcass_Construction" label="Carcass Construction">
			<COMBOBOX name="BU_Top_Panel" label="Top Panel" valuetype="IMAGE" default="Top_Rails" visible="TRUE" active="TRUE" image1="img/catalog/Top_Rails.PNG">
				<ITEM name="Top_Rails" value="Top_Rails" label="Rails" image1="img/catalog/Top_Rails.jpg" image2="img/catalog/.PNG" />
				<ITEM name="Full_Top" value="Full_Top" label="Full Top" image1="img/catalog/Full_Top.jpg" image2="img/catalog/.PNG" />
			</COMBOBOX>
		</GROUP>
	</PART>
	<PART name="PART_KITCHEN_Carcass_CONSTRUCTION_LOGIC">
		<COMMAND>"FIELD(CoreMaterial).Info1List(FIELD(CoreMaterialBrand).value)"</COMMAND>
		<IF>
			<EQ expr1="FIELD(CoreMaterial).info2" expr2="postLam" />
			<THEN>
				<COMMAND>"FIELD(CarcassLam).visible=TRUE"</COMMAND>
			</THEN>
			<ELSE>
				<COMMAND>"FIELD(CarcassLam).visible=FALSE"</COMMAND>
			</ELSE>
		</IF>
	</PART>


